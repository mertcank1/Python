kelime = input("Bir kelime girin: ")

if len(kelime) < 2:
    print("Kelimenin başı ve sonu aynı harfe sahip değil.")
elif kelime[0] == kelime[-1]:
    print("Kelimenin başı ve sonu aynı harfe sahiptir.")
