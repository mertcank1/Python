sayi = int(input("Bir sayı girin: "))
if sayi % 5 == 0 or sayi % 7 == 0:
    print(f"{sayi}, 5'e veya 7'ye bölünebilir.")
else:
    print(f"{sayi}, 5'e veya 7'ye bölünemez.")

