import random

rastgele_sayi = random.randint(1, 9)

while True:
    tahmin = int(input("1 ile 9 arasında bir sayı tahmin edin: "))

    if tahmin == rastgele_sayi:
        print("İyi tahmin")
        break
    else:
        print("Yanlış tahmin")

